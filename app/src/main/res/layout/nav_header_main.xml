<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="157dp"
        android:layout_height="73dp"
        android:contentDescription="AndroidLogo"
        app:srcCompat="@drawable/androidlogo" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="5dp"
        android:fontFamily="@font/alfa_slab_one"
        android:text="Trivia by Matthew Ellero. V 1.5.2"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="5dp"
        android:fontFamily="@font/alfa_slab_one"
        android:text="Songstar Search by Nouran Nouh"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="5dp"
        android:fontFamily="@font/alfa_slab_one"
        android:text="Car Database By Muhammad Ehsan Khan"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="5dp"
        android:fontFamily="@font/alfa_slab_one"
        android:text="Soccer Scores By Eric Myles"
        android:textSize="12sp" />
</LinearLayout>